  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "c3": "^0.6.7",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jsdom": "^12.1.0",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "radium": "^0.25.0",
    "react": "^16.5.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-animations": "^1.0.0",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-popup": "^0.9.3",
    "react-pose": "^3.3.6",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^1.5.0",
    "styled-components": "^3.4.9",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint --env browser,node --ext .jsx,.js src --fix",
    "testRun": "node scripts/test.js src --env=jsdom",
    "coverage": "node scripts/test.js src --coverage --env=jsdom"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/setupTestFramework.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/browserMocks.js"
